// Place your key bindings in this file to overwrite the defaults
// Commands begining with "-" are disable
[
    {
        "key"    : "ctrl+shift+p",
        "command": "workbench.action.showCommands"
    },
    {
        "key"    : "ctrl+shift+p",
        "command": "-workbench.action.showCommands"
    },
    {
        "key"    : "ctrl+shift+/",
        "command": "editor.action.blockComment",
        "when"   : "editorTextFocus && !editorReadonly"
    },
    {
        "key"    : "shift+alt+a",
        "command": "-editor.action.blockComment",
        "when"   : "editorTextFocus && !editorReadonly"
    },
    {
        "key"    : "ctrl+shift+numpad_subtract",
        "command": "editor.foldAll",
        "when"   : "editorTextFocus"
    },
    {
        "key"    : "ctrl+k ctrl+0",
        "command": "-editor.foldAll",
        "when"   : "editorTextFocus"
    },
    {
        "key"    : "ctrl+d",
        "command": "-editor.action.addSelectionToNextFindMatch",
        "when"   : "editorFocus"
    },
    {
        "key"    : "ctrl+shift+d",
        "command": "-workbench.view.debug"
    },
    {
        "key"    : "ctrl+alt+left",
        "command": "workbench.action.quickInputBack",
        "when"   : "inQuickOpen"
    },
    {
        "key"    : "alt+left",
        "command": "-workbench.action.quickInputBack",
        "when"   : "inQuickOpen"
    },
    {
        "key"    : "alt+left",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when"   : "terminalFocus"
    },
    {
        "key"    : "alt+left",
        "command": "-workbench.action.terminal.focusPreviousPane",
        "when"   : "terminalFocus"
    },
    {
        "key"    : "ctrl+alt+left",
        "command": "workbench.action.navigateBack"
    },
    {
        "key"    : "alt+left",
        "command": "-workbench.action.navigateBack"
    },
    {
        "key"    : "alt+right",
        "command": "workbench.action.terminal.focusNextPane",
        "when"   : "terminalFocus"
    },
    {
        "key"    : "alt+right",
        "command": "-workbench.action.terminal.focusNextPane",
        "when"   : "terminalFocus"
    },
    {
        "key"    : "alt+down",
        "command": "-workbench.action.terminal.focusNextPane",
        "when"   : "terminalFocus"
    },
    {
        "key"    : "ctrl+pagedown",
        "command": "workbench.action.nextEditor"
    },
    {
        "key"    : "ctrl+pageup",
        "command": "workbench.action.previousEditor"
    },
    {
        "key"    : "ctrl+shift+e",
        "command": "-workbench.view.explorer"
    },
    {
          "key"    : "alt",
          "command": "workbench.action.toggleMultiCursorModifier"
        , "when"   : "editorFocus"
    },
    {
        "key"    : "ctrl+delete",
        "command": "deleteWordRight",
        "when"   : "editorTextFocus && !editorReadonly"
    },
    {
        "key"    : "ctrl+backspace",
        "command": "-deleteWordRight",
        "when"   : "editorTextFocus && !editorReadonly"
    },
    {
        "key"    : "ctrl+shift+enter",
        "command": "editor.action.insertLineAfter",
        "when"   : "editorTextFocus && !editorReadonly && !suggestWidgetVisible && !findWidgetVisible"
    },
    {
        "key"    : "ctrl+alt+enter",
        "command": "editor.action.insertLineBefore",
        "when"   : "editorTextFocus && !editorReadonly && !suggestWidgetVisible && !findWidgetVisible"
    },
    {
        "key"    : "ctrl+enter",
        "command": "editor.action.insertLineBefore",
        "when"   : "editorTextFocus && !editorReadonly && !suggestWidgetVisible && !findWidgetVisible"
    },
    {
        "key"    : "ctrl+shift+alt+t",
        "command": "editor.action.refactor",
        "when"   : "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key"    : "ctrl+shift+r",
        "command": "-editor.action.refactor",
        "when"   : "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key"    : "ctrl+shift+alt+o",
        "command": "workbench.action.output.toggleOutput"
    },
    {
        "key"    : "ctrl+shift+u",
        "command": "-workbench.action.output.toggleOutput"
    },
    {
        "key"    : "ctrl+shift+u",
        "command": "editor.action.transformToUppercase"
    },
    {
        "key"    : "ctrl+shift+alt+u",
        "command": "editor.action.transformToLowercase"
    },
    {
        "key"    : "ctrl+k",
        "command": "-git.commitAll",
        "when"   : "!inDebugMode && !terminalFocus"
    },
    {
        "key": "f9",
        "command": "-workbench.action.debug.continue",
        "when": "inDebugMode"
    },
    {
        "key": "f9",
        "command": "workbench.action.debug.stepOver",
        "when": "inDebugMode"
    },
    {
        "key": "f8",
        "command": "-workbench.action.debug.stepOver",
        "when": "inDebugMode"
    },
    {
        "key": "ctrl+t",
        "command": "-git.sync"
    },
    {
        "key": "ctrl+shift+k",
        "command": "-git.pushTo",
        "when": "!inDebugMode && !terminalFocus"
    },
    {
        "key": "ctrl+alt+l",
        "command": "-editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key"    : "ctrl+alt+f",
        "command": "editor.action.formatDocument",
        "when"   : "editorHasDocumentFormattingProvider && editorTextFocus && !editorHasSelection && !editorReadonly"
    },
    {
        "key": "shift+alt+f",
        "command": "-editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+f",
        "command": "-editor.action.formatDocument.none",
        "when": "editorTextFocus && !editorHasDocumentFormattingProvider && !editorHasDocumentFormattingProvider && !editorReadonly"
    },
    {
        "key": "ctrl+alt+f",
        "command": "-workbench.action.toggleFullScreen"
    },
    {
        "key": "ctrl+alt+l",
        "command": "-editor.action.formatDocument",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k ctrl+f",
        "command": "-editor.action.formatSelection",
        "when": "editorHasDocumentSelectionFormattingProvider && editorHasDocumentSelectionFormattingProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+l",
        "command": "editor.action.formatSelection",
        "when": "editorHasDocumentFormattingProvider && editorHasSelection && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+alt+l",
        "command": "-editor.action.formatSelection",
        "when": "editorHasDocumentFormattingProvider && editorHasSelection && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+i ctrl+i",
        "command": "-extension.smartColumnIndenter.removeLineBreaks",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+i ctrl+j",
        "command": "-extension.smartColumnIndenter.indent2",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+alt+l",
        "command": "wwm.aligncode"
    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+shift+n",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+j",
        "command": "editor.action.joinLines"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.openRecent"
    },
    {
        "key": "ctrl+o ctrl+r",
        "command": "workbench.action.openRecent"
    },
    {
        "key": "alt+1",
        "command": "workbench.view.explorer"
    },
    {
        "key": "alt+2",
        "command": "workbench.view.search",
        "when": "!searchViewletVisible"
    },
    {
        "key": "alt+3",
        "command": "workbench.view.scm"
    },
    {
        "key": "ctrl+shift+d",
        "command": "workbench.view.debug"
    },
    {
        "key": "alt+4",
        "command": "workbench.view.debug"
    },
    {
        "key": "alt+5",
        "command": "workbench.view.extensions"
    },
    {
        "key": "alt+6",
        "command": "workbench.view.extension.sidebar-treeview"
    }
    ,{
        "key": "alt+7",
        "command": "workbench.view.extension.todo-tree-container"
    }
    ,{
        "key": "alt+8",
        "command": "workbench.view.extension.dockerView"
    }
    ,{
        "key": "alt+1",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarVisible"
    }
    ,{
        "key": "alt+2",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarVisible"
    }
    ,{
        "key": "alt+3",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarVisible"
    }
    ,{
        "key": "alt+4",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarVisible"
    }
    ,{
        "key": "alt+5",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarVisible"
    }
    ,{
        "key": "alt+6",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarVisible"
    }
    ,{
        "key": "alt+7",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarVisible"
    }
    ,{
        "key": "alt+8",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarVisible"
    }
    ,{
        "key": "ctrl+shift+q",
        "command": "editor.action.showHover",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+j",
        "command": "workbench.action.togglePanel"
    },
    {
        "key": "ctrl+j",
        "command": "-workbench.action.togglePanel"
    },
    {
        "key": "ctrl+shift+t",
        "command": "-workbench.action.reopenClosedEditor"
    },
    {
        "key": "ctrl+shift+r",
        "command": "workbench.action.openPreviousEditorFromHistory"
    },
    {
        "key": "ctrl+w",
        "command": "workbench.action.closeWindow",
        "when": "!editorIsOpen && !multipleEditorGroups"
    },
    {
        "key": "ctrl+w ctrl+q",
        "command": "workbench.action.closeActiveEditor",
        "when": "!editorIsOpen && !multipleEditorGroups"
    },
    {
        "key": "ctrl+f2",
        "command": "editor.action.selectHighlights"
    },
    {
        "key": "ctrl+f4",
        "command": "workbench.action.closeActiveEditor"
    },
    {
        "key": "f12",
        "command": "editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "f12",
        "command": "-editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "ctrl+f12",
        "command": "editor.action.goToImplementation",
        "when": "editorHasImplementationProvider && editorTextFocus && !isInEmbeddedEditor"
